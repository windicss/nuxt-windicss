{
  "name": "nuxt-windicss-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel --filter ./packages run dev",
    "build": "pnpm -r --parallel --filter ./packages run build",
    "prepack": "cp README.md packages/nuxt-windicss && cd packages/nuxt-windicss && yarn prepack",
    "lint": "eslint \"{packages,test}/**/*.{ts,vue,json,yml}\"",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/visitor-keys": "^5.18.0",
    "bumpp": "^7.1.1",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.13.0",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.9.3",
    "windicss": "^3.5.1"
  },
  "version": "2.2.8"
}
